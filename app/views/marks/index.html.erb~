<h1>Listing marks</h1>

<table>
  <tr>
    <th>Lat</th>
    <th>Lng</th>
    <th>Title</th>
  </tr>

<% for mark in @marks %>
  <tr>
    <td><%=h mark.lat %></td>
    <td><%=h mark.lng %></td>
    <td><%=h mark.title %></td>
    <td><%= link_to 'Show', mark %></td>
    <td><%= link_to 'Edit', edit_mark_path(mark) %></td>
    <td><%= link_to 'Destroy', mark, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

