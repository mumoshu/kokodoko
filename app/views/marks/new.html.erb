<h1>New mark</h1>

<%= render :partial => "edit_map" %>

<% form_for([@quiz,@mark]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :lat %><br />
    <%= f.text_field :lat %>
  </p>
  <p>
    <%= f.label :lng %><br />
    <%= f.text_field :lng %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', edit_quiz_path(@quiz) %>
