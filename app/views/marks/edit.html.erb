<h1>Editing mark</h1>

<%= render :partial => "edit_map" %>

<% form_for([@quiz,@mark]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :lat %><br />
    <%= f.text_field :lat %>
  </p>
  <p>
    <%= f.label :lng %><br />
    <%= f.text_field :lng %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [@quiz,@mark] %> |
<%= link_to 'Back', quiz_marks_path %>
